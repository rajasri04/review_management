<h1>Listing messages received by you </h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>From</th>
      <th>Content</th>
      <th>Created at</th>
      <th>Last Updated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @received_msg.each do |s| %>
      <tr>
        <td><%= s.from %></td>
        <td><%= s.content %></td>
        <td><%= s.created_at%></td>
        <td><%= s.updated_at%></td>
        <td><%= link_to 'Accept',notify_staff_path(@staff.id) %></td>
        <td><%= link_to 'Reject' %></td>

      </tr>
    <% end %>
  </tbody>
</table>

<br>
